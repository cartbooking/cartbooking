{% extends "admin/base.twig" %}
{% block title %}Publisher Details{% endblock %}
{% block content %}
    <div class="container-fluid">
      {% for message in app.session.flashbag.get('info') %}
          <div class="flash-info alert alert-success">{{ message }}</div>
      {% endfor %}
      {% include 'admin/publishers/search_form.twig' %}
        <div class="data" id="results"></div>
      {% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
      {{ form_start(form) }}
      {{ form_widget(form) }}
      {{ form_end(form) }}
    </div>
{% endblock %}
