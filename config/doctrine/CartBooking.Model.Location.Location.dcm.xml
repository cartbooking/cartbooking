<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="CartBooking\Model\Location\Location" table="locations">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="name" type="string" />
        <field name="volunteers" type="integer" />
        <field name="centre" type="string" />
        <field name="path" type="string" />
        <field name="markers" type="markers" />
        <field name="description" type="string" />
        <field name="zoom" type="integer" />
        <field name="capacity" type="integer" />
    </entity>
</doctrine-mapping>
