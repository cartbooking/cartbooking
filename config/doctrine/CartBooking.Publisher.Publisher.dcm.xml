<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="CartBooking\Publisher\Publisher" table="pioneers">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="firstName" column="first_name" type="string" />
        <field name="lastName" column="last_name" type="string" />
        <field name="gender" type="string"/>
        <field name="phone" type="string" />
        <field name="inactive" type="boolean" />
        <field name="password" type="string" />
        <field name="email" type="string" />
        <many-to-many field="relatives" target-entity="CartBooking\Publisher\Publisher">
            <join-table name="relationships">
                <join-columns>
                    <join-column name="publisher_id_1" referenced-column-name="id" />
                </join-columns>
                <inverse-join-columns>
                    <join-column name="publisher_id_2" referenced-column-name="id" />
                </inverse-join-columns>
            </join-table>
        </many-to-many>
    </entity>
</doctrine-mapping>
